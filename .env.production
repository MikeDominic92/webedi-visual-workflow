# Production Environment Variables for Netlify
# These should be set in Netlify's environment variables section

# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# AI Configuration - Direct APIs
# Set these in Netlify Dashboard -> Site Settings -> Environment Variables
# DO NOT commit actual API keys to version control
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here
REACT_APP_GROQ_API_KEY=your_groq_api_key_here
REACT_APP_ENABLE_AI_INTEGRATION=true
