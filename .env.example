# Application Configuration
REACT_APP_NAME=WebEDI Visual Workflow
REACT_APP_VERSION=1.0.0

# API Configuration
REACT_APP_API_URL=http://localhost:3001

# Supabase Configuration (Optional - for data persistence)
REACT_APP_SUPABASE_URL=your_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# AI Configuration - Unified OpenRouter API
REACT_APP_OPENROUTER_API_KEY=your_openrouter_api_key
REACT_APP_ENABLE_AI_INTEGRATION=true

# Legacy API Configuration (deprecated - now using OpenRouter)
# REACT_APP_GEMINI_API_KEY=your_gemini_api_key
# REACT_APP_GROQ_API_KEY=your_groq_api_key

# Feature Flags
REACT_APP_ENABLE_AI_INTEGRATION=false
REACT_APP_ENABLE_ANALYTICS=false

# Performance Configuration
REACT_APP_MAX_NODES=100
REACT_APP_ANIMATION_DURATION=300